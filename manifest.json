{
    "manifest_version": 3,
    "name": "HiredEasy",
    "version": "1.1",
    "description": "HiredEasy - Assign to Agent",
    "permissions": [
      "tabs",
      "storage",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "https://www.linkedin.com/*"
    ]
  }
